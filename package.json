{
  "name": "toprides",
  "version": "1.0.0",
  "private": true,
  "description": "Car booking system",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "lint": "tslint ./src/**/*.ts -t verbose",
    "e2e": "ng e2e",
    "zipdist": "cd dist/browser && zip -r dist.zip ./*",
    "build:dev": "ng build && npm run zipdist",
    "build:local": "ng build --prod --base-href http://52.66.235.25/toprides-angular/",
    "build:prod": "ng build --prod --base-href http://52.66.235.25/toprides-angular/",
    "deploy": "ngh --dir dist/browser --no-silent -r https://github.com/sksingh1995/toprides.git",
    "build:ssr": "npm run build:client-and-server-bundles && npm run webpack:server",
    "serve:ssr": "node dist/server.js",
    "build:client-and-server-bundles": "ng build --prod && ng run toprides:server",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors"
  },
